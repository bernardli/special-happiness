<%- include('manage-header') %>
        <div id="main">
            <!-- 草稿部分 -->
            <div class="posts">
                <h1 class="content-subhead">未发布文章</h1>
            </div>

            <!-- 已发布部分 -->   
            <div class="posts limit-post-content">
                <h1 class="content-subhead">最新文章</h1>
                <!--
                <%- include('components/limit-posts', { posts: posts }) %>
                -->  
            </div>
   
            <% if(posts.length==5){ %>
                <div class="menu-load">
                    <div class="item-load center" id="loadmore_posts">
                        <span class="text-load">----- 点击获取人生经验 ヾ(≧O≦)〃吼不吼啊~ -----</span>
                    </div>
                </div>
            <% } else { %>
                <div class="menu-load">
                    <div class="item-load center" id="no_posts">
                        <span class="text-load">----- 没有文章了/(ㄒoㄒ)/~~ -----</span>
                    </div>
                </div>
            <% } %>
        </div>
<%- include('manage-footer') %>